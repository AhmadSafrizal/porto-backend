{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "prisma/schema.prisma",
      "use": "@prisma/cli",
      "config": { "prismaVersion": "2.28" }
    }
  ],
  "hooks": {
    "postinstall": "npx prisma generate"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ]
}
